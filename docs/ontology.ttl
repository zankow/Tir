###  http://www.semanticweb.org/javi/ontologies/2025/3/tir#monument74
:monument74 rdf:type owl:NamedIndividual ,
                     :City ,
                     :Event ,
                     :Hotel ,
                     :Monument ,
                     :PublicTransport ;
            :tourLocatedIn :city03 ;
            :tourIdentifier "74"^^xsd:int ;
            :tourName "Palacio Madrid" ;
            :tourRatingValue 4.6 ;
